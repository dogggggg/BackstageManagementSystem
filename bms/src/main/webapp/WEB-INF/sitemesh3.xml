<?xml version="1.0" encoding="UTF-8"?>
<sitemesh>
	<mime-type>text/html</mime-type>
	<mime-type>application/vnd.wap.xhtml+xml</mime-type>
	<mime-type>application/xhtml+xml</mime-type>
	<mapping path="/page/**/*" decorator="/WEB-INF/layouts/default.jsp" />
	<mapping>
	   <path>/page/**/*</path>
	   <decorator>/WEB-INF/layouts/default.jsp</decorator>
	   <!--  <decorator>/WEB-INF/layouts/footer.jsp</decorator> -->
	</mapping>
	<mapping path="/js*" exclude="true" />
	<mapping path="/static*" exclude="true" />
	<mapping path="/page/index" exclude="true" />
	<mapping path="/page/login" exclude="true" />
	<content-processor>
		<tag-rule-bundle class="com.kdm.eip.common.support.ScriptTagRuleBundle" />
	</content-processor>

</sitemesh> 